#!/usr/bin/env python3
# encoding: utf-8

import os
import sys

FOLDER = os.path.dirname(os.path.realpath(__file__))
IMPORT = os.path.abspath(os.path.join(FOLDER, "..", "lib"))
if os.path.exists(IMPORT):
	sys.path.append(IMPORT)

from vyosextra import entry

if __name__ == '__main__':
	entry.vyos()

